<Window x:Class="Apskaita.Prisijungimas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Prisijungimas" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="Vartotojas" HorizontalAlignment="Left" Margin="75,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TabIndex="0"/>
        <Label Content="Vartotojas" HorizontalAlignment="Left" Margin="75,56,0,0" VerticalAlignment="Top" Width="120" Background="#66000000" FontWeight="Bold" FontSize="14" Foreground="White"/>
        <Label Content="Slaptazodis" HorizontalAlignment="Left" Margin="75,108,0,0" VerticalAlignment="Top" Width="120" Background="#66000000" FontWeight="Bold" FontSize="14" Foreground="White"/>
        <Button Content="Prisijungti" HorizontalAlignment="Left" Margin="75,182,0,0" VerticalAlignment="Top" Width="120" Height="37" Click="Button_Click" IsDefault="True" TabIndex="2"/>
        <PasswordBox x:Name="pass" HorizontalAlignment="Left" Margin="75,137,0,0" VerticalAlignment="Top" Width="120" TabIndex="1"/>

    </Grid>
</Window>
